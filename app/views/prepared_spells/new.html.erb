<div class="col-md-6 col-md-offset-1">
  <%= render(:partial => 'prepared_spells_table', :locals => {:spells => @spells, :spells_allowed => @spells_allowed}) %>
</div>
<div class="col-md-4">
  <%= form_for(@prepared_spell) do |f| %>
      <%= render(:partial => 'form', :locals => {:f => f}) %>

      <br><%= f.submit("Add spell", :class => 'btn btn-large col-md-8 col-md-offset-2') %>
  <% end %>
</div>
